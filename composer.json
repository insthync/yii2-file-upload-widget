{
    "name": "insthync/yii2-file-upload-widget",
    "description": "Blueimp file upload widget for the Yii framework (Forked from https://github.com/2amigos/yii2-file-upload-widget to update for latest Yii2)",
    "keywords": [
        "2amigos",
        "yii",
        "yii2",
        "yii 2",
        "widget",
        "upload"
    ],
    "type": "yii2-extension",
    "license": "BSD-3-Clause",
    "homepage": "https://github.com/insthync/yii2-file-upload-widget",
    "authors": [
        {
            "name": "2amigOS! Consulting Group",
            "email": "hola@2amigos.us",
            "homepage": "http://2amigos.us",
            "role": "Developer"
        },
        {
            "name": "Insthync",
            "role": "Developer"
        }
    ],
    "require": {
        "yiisoft/yii2": "~2.0.45",
        "yiisoft/yii2-bootstrap5": "~2.0.2",
        "insthync/yii2-gallery-widget": "^1.0",
        "bower-asset/blueimp-file-upload": "~10.32"
    },
    "require-dev": {
        "phpunit/phpunit": "~9.5.0",
        "roave/security-advisories": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "dosamigos\\fileupload\\": "src"
        }
    },
    "config": {
        "fxp-asset": {
            "installer-paths": {
                "npm-asset-library": "vendor/npm",
                "bower-asset-library": "vendor/bower"
            }
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
